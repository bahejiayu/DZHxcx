.tit_{
  padding: 0 25rpx;
  height: 65rpx;
  line-height: 65rpx;
  font-size: 28rpx;
  color: #828282;
}
page{
  background-color: #f7f7f7;
}
.inp_{
  padding:0 25rpx ;
  background-color: #fff;
  height: 80rpx;
}
.list_img image{
  width: 225rpx;
  height: 225rpx;
  margin-right: 10rpx;
  position: relative;
  border: 1px solid #dcdcdc;
}
.con>view{
  float: left;
}
.con{
  overflow: hidden;
}
.list_img image view{
  position: absolute;
  bottom: 0;
  right: 0;
  font-size: 20rpx;
  padding: 5rpx 10rpx;
  background-color: #ff3153;
  color: #fff;
}
.inp_ input{
  width: 100%;
  height: 100%;
  font-size: 28rpx;
}
textarea{
  width: 100%;
  height: 300rpx;
  background-color: #fff;
  box-sizing: border-box;
  padding: 25rpx;
  font-size: 28rpx;
}
image{
  width: 225rpx;
  margin-left: 25rpx;
  height:225rpx ;
}