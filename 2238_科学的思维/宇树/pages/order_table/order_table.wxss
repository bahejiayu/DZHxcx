page{
  background-color: #f7f7f7;
}
.address_info{
  background-color: #fff;
  padding: 20rpx 30rpx;
  border-top: 1px solid #dcdcdc;
  position: relative;
  padding-right: 80rpx;
  padding-bottom: 40rpx;
}
.ico_address{
      position: absolute;
      width: 18rpx;
      height:25rpx; 
      top:50%;
      transform: translateY(-50%);
      right: 30rpx;

}
.comp_right image{
  width: 30rpx;
  height: 30rpx;
  margin-right:5rpx;
  vertical-align: baseline;

}
.add_detail{
  font-size:26rpx ;
  color:#333333 ;
}
.add_l{
  float: left;
}
.add_r{
  float: right;
}
.top{
  overflow: hidden;
  margin-bottom: 20rpx;
}
.order_goods{
  margin: 20rpx 0 0 0;
  background-color: #fff;
}
.order_head{
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #dcdcdc;
  padding: 0 30rpx;
}
.head_l{
  font-size: 26rpx;
  float: left;
  color: #9d2427;
}
.head_r{
  float: right;
  color:#828282 ;
  font-size: 24rpx;
}
.goods_list{
  position: relative;
  padding-left: 220rpx;
  font-size: 25rpx;
  border-bottom: 1px solid #dcdcdc;
  color: #333333;
  padding-right: 160rpx;
  z-index: 1100;
  /* height: 200rpx; */
}
.goods_img{
    width: 200rpx;
    height: 100%;
    position: absolute;
    left: 0;

}
.goods_img image{
  width: 100%;
  height: 100%;

}
.nub{
  position: absolute;
  right: 30rpx;
  top: 20rpx;
}
.goods_txt{
  padding: 20rpx 0;
}
.goods_price{
  margin-top: 53rpx;
}
.name{
  /* overflow: hidden; */
  overflow:hidden;
text-overflow:ellipsis;
display:-webkit-box;
-webkit-box-orient:vertical;
-webkit-line-clamp:2;


}
.goods_btn{
  padding: 23rpx 0;
  text-align: right;
  margin-right: 30rpx;
}
.goods_btn navigator{
  display: inline-block;
  padding:15rpx 20rpx;
  font-size: 24rpx;
  color: #ab262c;
  border: 1px solid #ab262c;
  border-radius: 10rpx;
  margin-left: 30rpx;
}
.goods_btn .c{
  border: 1px solid #828282;
  color: #828282;
}
.order_bottom{
  border-top: 1px solid #dcdcdc;
  
  padding: 25rpx 30rpx;
  background-color: #fff;
  margin-bottom: 40rpx;
}
.item_l{
  float: left;
  font-size: 28rpx;
}
.item_r{
  float: right;
   font-size: 28rpx;
   color: #333333;

}
.item_b{
  /* margin-bottom: 20rpx; */
  overflow: hidden;
}

.to_l{
  float:left;
  font-size:30rpx ;
  color: #333333;
}
.to_r{
  float: right;
  color: #9d2427;
  font-size:30rpx ;
}
.total_{
  padding-top: 30rpx;
  overflow: hidden;
}
.order_time{
  background-color: #fff;
  padding: 20rpx 30rpx;
  margin-bottom: 100rpx;
  overflow: hidden;
  
}
.time_l{
  float: left;

}
.time_r{
  float: right;
}
.order_time view{
  font-size:26rpx ;
  color:#828282 ;
}
.order_time>view{
  margin-top: 20rpx;
  
  overflow: hidden;
}
.order_time>view:first-child{
  margin-top: 0;
}
.bian_{
  width: 100%;
  position: absolute;
  bottom: 0;
}
.comp_{
  margin: 20rpx 0;
  background-color: #fff;
  padding: 0 25rpx;
}
.comp_item{
  height: 80rpx;
  line-height: 80rpx;
}
.comp_item{
  font-size: 26rpx;
  }
  .comp_left{
    float: left;
  }
  .comp_right{
    float: right;
  }
  .comp_item:first-child{
    border-bottom: 1px solid #eeeeee;
  }
  .goods_container{
    padding-left: 30rpx;
  }
  .goods_list:last-child{
    border-bottom: none;
  }
  .bottom_fixed{
  width: 100%;
  display: flex;
  position: fixed;
  height: 98rpx;
  border-top: 1px solid #dcdcdc;
  bottom: 0;
  background-color: yellow;
}

.bottom_fixed view{
  align-items: center;
  justify-content: center;
}
.fix_car{
  flex: 2;
  height: 100%;
  background-color: #fff;
  border-right: 1px solid #dcdcdc;
}
.once_{
  flex: 2;
  display: flex;
  font-size: 28rpx;
  color: #fff;
  background-color: #ab262c;
  
}
.add_car{
  font-size: 28rpx;
  flex: 1;
  display: flex;
  color: #fff;
  background-color: #9d2427;
  align-items:center;
justify-content:center;

  
}
.fix_car view{
  text-align: right;
  font-size: 24rpx;
  /* flex: 1; */
  margin-right:20rpx!important ;
}
.fix_car view:first-child{
  font-size:26rpx ;
  color: #828282;
  margin: 10rpx 0;
}
.fix_car view:last-child{
    font-size:30rpx ;
    color: #333333;
}
.fix_car view:last-child text{
  color: #9d2427;
}
.mdz{
  text-align: center;
  color: #828282;
  font-size: 26rpx;
}
.mdz image{
  margin-bottom: 10rpx;
  width:56rpx ;
  height: 62rpx;
}
.dw{
  margin-bottom: 30rpx;
}
.total_line{
  padding: 0 25rpx;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  font-size:26rpx ;
  margin-bottom: 30rpx;
  color: #333333;
}
.total_line view:first-child{
  float: left;
}

.total_line view:last-child{
  float: right;
}
.index_item{
    padding: 20rpx 25rpx;
    padding-left: 250rpx;
    position: relative;
    height: 200rpx;
    border-bottom: 1px solid #dcdcdc;
    background-color: #fff;
}

.index_left{
  width:200rpx ;
  height: 200rpx;
  position: absolute;
  left: 25rpx;
  top: 20rpx;
}
.index_left image{
  width: 100%;
  height: 100%;
}
.goods_tit{
  font-size: 30rpx;
  color: #333333;
}


.goods_info{
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #828282;
}
.goods_info view{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.goods_info view:first-child{
  margin-bottom: 9rpx;
}
.index_right{
  height: 100%;
  position: relative;
}
.goods_price{
  font-size:36rpx ;
  color: #febd00;
  bottom: 0;
  position: absolute;
}
.index_item navigator{
  height: 100%;
  display: block;
}
.jg_tip{
  padding: 0 25rpx;
  font-size:28rpx ;
  color: #828282;
  margin-top: 45rpx;
}













