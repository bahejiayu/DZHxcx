page{
  border-top: 1px solid #dcdcdc;
  background-color: #f7f7f7;
}
.jo_item{
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #dcdcdc;
  background-color: #fff;
  padding-right: 20rpx;
  position: relative;
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.text{
    /* position: absolute; */
    display: inline-block;
    width: 15rpx;
    height: 15rpx;
    background-color: red;
    margin-left: 10rpx;
    border-radius: 50%;
}
.jo_item navigator{
  display: block;
  width: 100%;
  height: 100%;
  position: relative;
  padding:  0 25rpx;
box-sizing: border-box;  
}
.jo_item image{
  width: 10rpx;
  height: 17rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 25rpx;
}