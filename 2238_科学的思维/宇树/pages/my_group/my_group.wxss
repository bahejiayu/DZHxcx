.group_tit{
  padding: 0 25rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #f7f7f7;
  border-top: 1px solid #dcdcdc;
  font-size: 32rpx;
  color: #333333;
}

.group_box{
  padding: 0 12.5rpx;
  padding-top: 50rpx;
  
}
.group_item{
  float: left;
  width: 25%;
  text-align: center;
  font-size:28rpx ;
  color:#333333 ;
  box-sizing: border-box;
  padding: 0 12.5rpx;
  margin-bottom: 50rpx;
  overflow: hidden;
  white-space: normal;
  text-overflow: ellipsis;
}
.group_item image{
  width:120rpx ;
  height: 120rpx;
  border-radius: 50%;
  margin-bottom: 10rpx;
}


























